{
	"manifest_version": 2,
	"name": "reaction",
  	"version": "1.1",
	"author": "Matt Gaikema",
	"default_locale": "en",
	"description": "Downloads a Facebook reaction for use anywhere.",
	"homepage_url": "https://github.com/TexAgg/reactions-anywhere",
	"permissions": [
    	"activeTab",
		"downloads"
  	],
	"browser_action": {
		"default_title": "Reaction",
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"img/angry.png",
		"img/haha.png",
		"img/like.png",
		"img/love.png",
		"img/sad.png",
		"img/wow.png"
	]
}